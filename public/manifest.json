{
  "name": "seeyoulater",
  "description": "Tab snoozer extension.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "storage", "unlimitedStorage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
